.container {
  background-color: #fff;
}

.topBg {
  width: 100%;
  height: 130rpx;
}

.item {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 40rpx;
  border-bottom: 1px solid #ececec;
  font-size: 30rpx;
  color: #0D8FEF
}

.btn {
  width: 112rpx;
  height: 50rpx;
  border: 1px solid #0d8fef;
  border-radius: 4px;
  text-align: center;
  line-height: 50rpx;
  font-size: 24rpx;
  color: #0d8fef;
}

.success {
  width: 112rpx;
  height: 50rpx;
  border-radius: 4px;
  text-align: center;
  line-height: 50rpx;
  border: 1px solid #999;
  font-size: 24rpx;
  color: #333;
}

.storeName {
  font-weight: bold;
}

.finish_Name {
  color: #333;
}

.finish_time{
  color: #999999;
}

.finish_btn{
  border-color: #999999;
  color: #333333;
}

.nodata{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  color: #666666;
  font-size: 32rpx;
  text-align: center;
}

.noOrder{
  width: 140rpx;
  height: 180rpx;
}

.msg{
  margin-top: 8rpx;
}